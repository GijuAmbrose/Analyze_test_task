<p id="notice"><%= notice %></p>

<div class="container">
  <h1 class="mb-4 mt-5">Sectors</h1>

  <table class="table table table-bordered">
    <thead>
      <tr>
        <th scope="col">Sector name</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @sectors.each do |sector| %>
        <tr>
          <td><%= sector.sector_name %></td>
          <td><%= link_to 'Show', sector %></td>
          <td><%= link_to 'Edit', edit_sector_path(sector) %></td>
          <td><%= link_to 'Destroy', sector, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Sector', new_sector_path %>
</div>
