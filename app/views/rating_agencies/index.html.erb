<p id="notice"><%= notice %></p>

<div class="container">
  <h1 class="mb-4 mt-5">Rating Agencies</h1>

  <table class="table table table-bordered">
    <thead>
      <tr>
        <th scope="col">Rating agency name</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @rating_agencies.each do |rating_agency| %>
        <tr>
          <td><%= rating_agency.rating_agency_name %></td>
          <td><%= link_to 'Show', rating_agency %></td>
          <td><%= link_to 'Edit', edit_rating_agency_path(rating_agency) %></td>
          <td><%= link_to 'Destroy', rating_agency, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Rating Agency', new_rating_agency_path %>
</div>
