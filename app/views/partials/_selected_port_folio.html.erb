<table class="table table-bordered mt-3">
  <thead>
  <tr>
      <th scope="col">Sl. No.</th>
      <th scope="col">ISIN</th>
      <th scope="col">Nomenclature</th>
      <th scope="col">Date of Issue</th>
      <th scope="col">Date of Maturity</th>
      <th scope="col">Trade Price</th>
      <th scope="col">Interest %</th>
      <th scope="col">YTM %</th>
      <th scope="col">Select</th>
  </tr>
  </thead>
  <tbody>
    <% index = 0%>
  <% table_datas.each do |key, value|%>
    <tr class="port-folio-inner-row-<%= index+=1%>">
      <th scope="row" class=""><%= index%></th>
      <td class="port-folio-inner-isin" value="<%= value[0] %>"><%= value[0] %></td>
      <td class="port-folio-inner-nomenclature" value="value[1]"><%=value[1]%></td>
      <td class="port-folio-inner-issue-dt" value="<%= value[2] %>"><%= value[2] %></td>
      <td class="port-folio-inner-mature-dt" value="<%= value[3] %>"><%= value[3] %></td>
      <td class="port-folio-inner-trade-price" value="<%= value[4] %>"><%= value[4] %></td>
      <td class="port-folio-inner-freq" value="<%= value[5] %>"><%= value[5] %></td>
      <td class="port-folio-inner-ytm" value="<%= value[6] %>"><%=value[6] %></td>
      <td class="port-folio-inner-select-td"><label class="btn btn-color">
        <input type="checkbox" name="port-folio-check" class="port-folio-inner-select" id="c-hidden" value="<%= index%>"/> Select
      </label></td>
    </tr>
  <% end %>
    </tbody>
</table>

