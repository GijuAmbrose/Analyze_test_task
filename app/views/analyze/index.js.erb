<% if params[:ajax_call].present? &&  params[:ajax_call] == "true" %>
  $(".analyse-tab-data").html("<%= escape_javascript(render partial: 'partials/table_results', locals: { table_datas: @analyse_data } ) %>")
<% elsif params["values"].present?%>
	$(".selected_values").html("<%= escape_javascript(render partial: 'partials/selected_table', locals: { table_datas: @values } ) %>")
<% end %>